.Ftabber {
  width: 100%;
  height: 100rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px -1px 10px 0px rgba(238, 238, 238, 1);
  padding: 0 40rpx;
  box-sizing: border-box;
  position: fixed;
  left: 0;
  bottom: 0;
}

.Ftabber_item {
  text-align: center;
  width: 114rpx;
  color: #333;
  font-size: 20rpx;
}
.Ftabber_item>image.Flogo1{
  width:54rpx;
height:46rpx;
margin-bottom: 7rpx;
}
.Ftabber_item>image.Flogo2{
  width:48rpx;
height:48rpx;
margin-bottom: 5rpx;
}
.Ftabber_item>image.Flogo3{
  width:45rpx;
height:45rpx;
margin-bottom: 8rpx;
}
.Ftabber_item>image.Flogo4{
  width:48rpx;
height:44rpx;
margin-bottom: 9rpx;
}
.Ftabber_item>image.Fmid{
width:114rpx;
height:114rpx;
position: relative;
top: -20rpx;
}